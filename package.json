{
  "name": "nazrin",
  "version": "3.0.0",
  "private": true,
  "author": "oame <oame@oameya.com> (http://oameya.com/)",
  "license": "MIT",
  "homepage": "http://nazr.in",
  "engines": {
    "node": "8.1.4"
  },
  "main": "app.js",
  "scripts": {
    "postinstall": "cd client && npm install",
    "start": "node ./bin/www",
    "flow": "flow",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "run-p test:*",
    "test:server": "nyc ava",
    "watch": "run-p watch:*",
    "watch:server": "nodemon ./bin/www",
    "watch:client": "cd client && npm start"
  },
  "babel": {
    "plugins": [
      "babel-plugin-transform-flow-strip-types"
    ]
  },
  "ava": {
    "require": "babel-register"
  },
  "dependencies": {
    "base62": "^1.1.1",
    "body-parser": "^1.16.1",
    "corser": "^2.0.1",
    "express": "^4.14.1",
    "mongoose": "^4.9.8",
    "mongoose-sequence": "^3.2.0",
    "morgan": "^1.7.0",
    "raven": "^2.0.0",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "flow-bin": "^0.49.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "nyc": "^11.0.3",
    "supertest": "^3.0.0"
  }
}
